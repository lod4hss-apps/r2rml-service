@prefix : <http://geovistory.org/resource/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ontome: <https://ontome.net/ontology/> .


<urn:MAPID-1483135-entities> a rr:TriplesMap;


  rr:SQLQuery """
      select pk_entity, class_uri, entity_label
        from rdf.v_ontop_entity_preview_corr
        where fk_project = 1483135
      """;

  rr:subjectMap [
      rr:template "http://geovistory.org/resource/i{pk_entity}";
      rr:termType rr:IRI
    ];
    rr:predicateObjectMap [ a rr:PredicateObjectMap;
      rr:objectMap [ 
          rr:column "class_uri";
          rr:termType rr:IRI
        ];
      rr:predicate rdf:type
    ], 
    [ a rr:PredicateObjectMap;
      rr:objectMap [
          rr:column "entity_label";
          rr:termType rr:Literal
        ];
      rr:predicate rdfs:label
    ];
   .
